# ValoRPC Environment Configuration
# Copy this file to .env and fill in your values

# API Server Configuration
# Set this to your Railway API URL to enable dashboard status updates
# When running locally: http://localhost:8000 or http://192.168.x.x:8000
# When deployed on Railway: https://web-production-xxxxx.up.railway.app
VALORPC_API_URL=http://localhost:8000

# Optional: Override the default port for the API server (Railway sets this automatically)
# PORT=8000

# Optional: Flask debug mode (set to False in production)
# FLASK_ENV=development
